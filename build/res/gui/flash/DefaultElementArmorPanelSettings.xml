<mmps>
	<config>
		<enableEffArmor>true</enableEffArmor>    <!-- Displays effective armor calculation. ** vehicles missing arc values in the "vehiclesData" are calcualted without armor slope. -->
		<cameraAngle>true</cameraAngle> <!-- effective armor calculation follows the camera, if false it follow your turret -->
		<enableRulerWhenTD>true</enableRulerWhenTD>    <!-- Displays GunArc Critical Angle Ruler for TD's. -->
		<enableRulerWhenSPG>true</enableRulerWhenSPG>    <!-- Displays GunArc Critical Angle Ruler for SPG's. -->
		<ordinateWhenTank>0.80</ordinateWhenTank>    <!-- Vertical placement of Effective armor calculation ( In percentage of lower half vertical screen resolution ). -->
		<ordinateWhenTD>0.70</ordinateWhenTD>    <!-- Vertical placement of TD GunArc Ruler ( where 0 is screen center and 1 its bottom ). -->
		<protractorOffsetWhenTD>43</protractorOffsetWhenTD>
		<ordinateWhenSPG>0.0</ordinateWhenSPG>    <!-- placement of SPG GunArc Ruler. ( In percentage of lower half vertical screen resolution ) Recommend 0 or ~0.7-0.1 -->
		<m2pix>8.2</m2pix>    <!-- Distance to Pixels ratio, Default is 1m = 8.2px. -->
		<xSpacing>0</xSpacing> <!-- GunArc Ruler degre dash'es spacing, default is 0 adaptive. Recommended setting for fixed spacing is 16 or 32. -->
		<funcKey>17</funcKey>    <!-- Key to trigger extra function, currently no extra functions yet. -->
		<debug>false</debug>    <!-- warn when no arc found(red dot on panel), print outs etc, etc -->
		<penIndicator>true</penIndicator> <!-- very basic indicator that targets pen compared to eff front armor. -->
		<debugRenderHitPosKey>66</debugRenderHitPosKey>
		<posUpKey>78</posUpKey>
		<posDownKey>74</posDownKey>
		<posDefaultKey>55</posDefaultKey>
		<hitHullHeight>0.7</hitHullHeight>
		<useTargetGunY>true</useTargetGunY>
		<rayLength>8.0</rayLength>
		<gunPitchHitAngle>true</gunPitchHitAngle>
		
