    <!-- "name" Vehicle name. Ingame-name of player's vehicle icon in lower case without file name suffix -->
    <!-- "type" Vehicle type tags: Tank/TD/SPG, ** type="0" to not display GunArc Ruler. -->
    <!-- "leftArc" Gun left angle limit. -->
    <!-- "rightArc" Gun right angle limit. -->
